<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="script.return.kodi" name="@Return To Kodi" version="1.0.0" provider-name="MiCHaEL">
	<requires>
		<import addon="xbmc.python" version="2.14.0"/>
	</requires>
	<extension point="xbmc.python.script" library="addon.py">
		<provides>executable</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<summary lang="en">Return to Kodi</summary>
		<description lang="en">Return to Kodi closing Chrome Web Browser</description>
		<license>GNU General Public License, v2</license>
		<language></language>
		<source>https://github.com/michaelnpsp/kodiaddons</source>
		<website>nospam</website>
		<email>nospam@nospam.com</email>
		<assets>
			<icon>icon.png</icon>
		</assets>
		<news></news>
	</extension>
    <extension point="xbmc.addon.repository" name="MiCHaEL Personal Repository">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/michaelnpsp/kodiaddons/master/repository/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/michaelnpsp/kodiaddons/master/repository/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/michaelnpsp/kodiaddons/master/repository/zips/</datadir>
        </dir>
    </extension>
</addon>
